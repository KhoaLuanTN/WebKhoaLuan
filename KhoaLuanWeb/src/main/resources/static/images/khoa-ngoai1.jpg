*{font-family:Roboto-Regular,sans-serif;box-sizing:border-box}#footer-intro-cv-job{padding-top:10px}#footer-intro-cv-job{z-index:1000;display:none;width:100%;position:fixed;bottom:0;left:0;min-height:47px;background:#032456;opacity:.9;padding:0}.text-white{color:#fff}#footer-intro-cv-job>a.slide-down,#footer-intro-cv-job>a.slide-up{color:#aaa;opacity:.8;font-size:12px;line-height:12px;right:10px;top:3px;position:absolute}#footer-intro-cv-job>a.slide-up{display:none}#footer-intro-cv-job .container{padding-left:0;padding-right:0;width:82%;margin:0 auto;background:0 0}.text-center{text-align:center}#footer-intro-cv-job .bottom-buttons{margin-bottom:9px;padding-top:0}#footer-intro-cv-job p{padding-top:4px;margin:9px 0 0;font-weight:700;font-size:20px;line-height:28px;float:left}#footer-intro-cv-job .intro-headline{margin-right:20px}#footer-intro-cv-job .btn{width:169px;height:33px}.hide{display:none}#footer-intro-cv-job .btn-topcv-primary{background:#ffb229;color:#000;padding:6px 60px}#footer-intro-cv-job .btn{font-weight:700;text-align:center;border-radius:10px;font-size:14px}#footer-intro-cv-job .btn-danger{color:#000;background:#fff600;border-color:#d43f3a;padding:6px 34px}.container:after,.container:before{display:table;content:" "}#footer-intro-cv-job .slide-down .fa-angle-down::before{content:'';background:url(/images/icons/close_btndo.png) no-repeat;background-size:100% 100%;width:12px;height:12px;display:none}.collapsed .fa-angle-up::before{content:'';background:url(/images/icon_fa_angle_up.png) no-repeat;width:19px;height:12px;display:none}#footer-intro-cv-job.collapsed .slide-up{display:none}#footer-intro-cv-job.collapsed .intro-headline{width:52%;float:left;font-size:20px;text-align:left;padding-top:15px;margin-top:0}#footer-intro-cv-job.collapsed .intro-items{display:none}#footer-intro-cv-job.collapsed .bottom-buttons{float:left;margin-top:8px;text-align:left}#footer-intro-cv-job.collapsed img{display:none}a{text-decoration:none}#footer-intro-cv-job .close_intro{margin-left: -50px;font-family: Roboto-Medium;cursor: pointer;}@media(max-width:1024px){#footer-intro-cv-job.collapsed .bottom-buttons,#footer-intro-cv-job.collapsed .intro-headline{width:100%;text-align:center}}@media(min-width:1367px){#footer-intro-cv-job{min-height:60px}#footer-intro-cv-job .btn{width:174px;height:42px}#footer-intro-cv-job .btn-topcv-primary{padding:10px 60px}#footer-intro-cv-job .btn-danger{padding:10px 34px}}@media(max-width:768px){#footer-intro-cv-job.collapsed .bottom-buttons,#footer-intro-cv-job.collapsed .intro-headline{width:100%;text-align:center}}@media(max-width:415px){#footer-intro-cv-job,#footer-intro-cv-job .container{width:100%;min-width:100%}#footer-intro-cv-job img{display:none}#footer-intro-cv-job .slide-down,#footer-intro-cv-job.collapsed .slide-up{left:10px;}.collapsed .intro-headline{display:none}#footer-intro-cv-job.collapsed .intro-headline{width:100%;font-size:18px;line-height:20px;text-align:center;padding-top:20px}#footer-intro-cv-job p{width:100%;font-size:18px;line-height:20px;text-align:left;padding-top:20px}#footer-intro-cv-job li{font-size:16px;line-height:20px;font-weight:400}#footer-intro-cv-job .btn-topcv-primary{padding:9px 40px;width:120px}#footer-intro-cv-job .btn-danger{padding:9px 15px;width:131px;margin-left:15px!important}#footer-intro-cv-job.collapsed .bottom-buttons{text-align:left;margin-top:10px;margin-bottom:10px}#footer-intro-cv-job.collapsed .btn-danger{margin-left:15px!important}#footer-intro-cv-job .text-center{width:96%;margin:0 auto}.close_intro{display: none;}}
</style>

<div id="footer-intro-cv-job" class="text-white text-center-sm">
  <a class="slide-down"><span class="fa fa-angle-down"></span></a>
  <a class="slide-up"><span class="fa fa-angle-up"></span></a>
  <div class="container">
    <div class="text-center">
      <p class="close_intro">&#x2715</p>
      <p class="intro-headline">Tạo CV miễn phí và tìm công việc mơ ước với timviec365.vn</p>
      <p class="bottom-buttons">
        <a href="https://timviec365.vn/cv-xin-viec" class="btn btn-topcv-primary">Tạo CV</a>
        <a href="https://timviec365.vn/" class="btn btn-danger" style="margin-left: 20px;">Tìm việc làm</a>
      </p>
    </div>
  </div>
</div>

<script async>
  $(document).ready(function () {
    $(window).scroll(function () {
      if ($('#footer-intro-cv-job').hasClass('hide')) return true;
      var position = $(this).scrollTop();
      if (position > 400) {
        $('#footer-intro-cv-job').addClass('collapsed');
        $('#footer-intro-cv-job').slideDown(300);
      } else {
        $('#footer-intro-cv-job').slideUp(300);
      }
    });
    $(".close_intro").click(function(){
      $("#footer-intro-cv-job").remove();
    });
  });
</script>

</body>
</html>

